{
  "name": "pip-webui2-pictures",
  "version": "2.0.0-rc.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "build:lib": "ng build pip-webui2-pictures && npm run build:lib-styles && npm run build:lib-md",
    "build:lib-styles": "scss-bundle -c ./projects/pip-webui2-pictures/scss-bundle.config.json",
    "build:lib-md": "cpx \"{LICENSE,README.md,CHANGELOG.md}\" dist/pip-webui2-pictures",
    "test": "ng test",
    "test:lib": "ng test  pip-webui2-pictures",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-pictures",
    "e2e": "ng e2e",
    "package": "npm run build:lib && cd dist/pip-webui2-pictures && npm pack",
    "deploy": "ng build --prod --base-href https://pip-webui2.github.io/pip-webui2-pictures/ && ngh --dir dist/pip-webui2-pictures-app"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@ngx-translate/core": "^13.0.0",
    "core-js": "^3.6.5",
    "ngx-webcam": "^0.3.0",
    "pip-webui2-behaviors": "^2.0.0-rc.1",
    "pip-webui2-themes": "^2.0.0-rc.3",
    "rxjs": "~6.6.3",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "^10.1.4",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.11.2",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "~6.0.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.1.2",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.1.2",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  }
}
